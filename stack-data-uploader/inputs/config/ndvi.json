{
    "name": "ndvi",
    "database": "postgres",
    "workspace": "twa",
    "skip": false,
    "datasetDirectory": "ndvi",
    "dataSubsets": [
        {
            "name": "raw_contour",
            "skip": false,
            "type": "vector",
            "subdirectory": "raw_contour",
            "ogr2ogrOptions": {
                "sridIn": "EPSG:4326"
            },
            "geoServerSettings": {
                "virtualTable": {
                    "name": "raw_contour",
                    "sql": "SELECT * FROM raw_contour",
                    "geometry": {
                        "name": "wkb_geometry",
                        "type": "MultiPolygon",
                        "srid": 4326
                    }
                }
            }
        },
        {
            "name": "raster_2019",
            "skip": false,
            "type": "raster",
            "subdirectory": "raster_2019",
            "geoServerSettings": {
                "layerSettings": {
                    "defaultStyle": "raster"
                }
            }
        },
        {
            "name": "raster_dump_2019",
            "skip": false,
            "type": "vector",
            "subdirectory": "raster_dump_2019",
            "sql": "@/inputs/data/ndvi/raster_dump_2019/raster_dump_2019.sql",
            "additionalMetadata": {
                "prefixes": {
                    "exposure": "https://www.theworldavatar.com/kg/ontoexposure/",
                    "dcterms": "http://purl.org/dc/terms/",
                    "dcat": "http://www.w3.org/ns/dcat#",
                    "xsd": "http://www.w3.org/2001/XMLSchema#"
                },
                "triplePatterns": "?dataSubset exposure:hasValueColumn 'val'; exposure:hasGeometryColumn 'wkb_geometry'; exposure:hasAreaColumn 'area'; a exposure:AreaWeightedDataset."
            }
        }
    ]
}