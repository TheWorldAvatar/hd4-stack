                },
                "triplePatterns": "?dataSubset rdfs:label \"Greenspace 2023 (NParks)\"; a exposure:GreenspacePolygonDataset; dcterms:temporal [ a dcterms:PeriodOfTime; dcat:startDate \"2023-01-01\"^^xsd:date; dcat:endDate \"2023-12-31\"^^xsd:date]"
            }
