{
    "ServiceSpec": {
        "Name": "exposure-feature-info-agent",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/theworldavatar/exposure-feature-info-agent:1.0.0",
                "Env": [
                    "JAVA_OPTS=-agentlib:jdwp=transport=dt_socket,server=y,suspend=y,address=*:5005"
                ],
                "Configs": [
                    {
                        "ConfigName": "blazegraph"
                    },
                    {
                        "ConfigName": "postgis"
                    },
                    {
                        "ConfigName": "ontop"
                    }
                ],
                "Secrets": [
                    {
                        "SecretName": "postgis_password"
                    }
                ]
            }
        },
        "EndpointSpec": {
            "Ports": [
                {
                    "Name": "web",
                    "Protocol": "tcp",
                    "TargetPort": "5005",
                    "PublishedPort": "5005"
                }
            ]
        }
    },
    "endpoints": {
        "rest": {
            "url": "http://localhost:8080/ExposureFeatureInfoAgent/",
            "externalPath": "/exposure-feature-info-agent/"
        }
    }
}